<?xml version="1.0" encoding="UTF-8" ?>
<manifest>
  <remote name="origin" fetch="https://github.com/ivladak/" />
  <remote name="pi" fetch="ssh://pi@pi/home/pi/git/" />

  <remote name="rupa" fetch="https://github.com/rupa/" />
  <remote name="vim-scripts" fetch="https://github.com/vim-scripts/" />
  <remote name="raimondi" fetch="https://github.com/Raimondi/" />
  <remote name="tpope" fetch="https://github.com/tpope/" />
  <remote name="kopischke" fetch="https://github.com/kopischke/" />
  <remote name="alphapapa" fetch="https://github.com/alphapapa/" />

  <default remote="origin" revision="master" />

  <project name=".dotfiles" path="." />

  <project name="a.vim" remote="vim-scripts"
           path=".vim/bundle/a.vim" />
  <project name="delimitMate" remote="raimondi"
           path = ".vim/bundle/delimitMate" />
  <project name="vim-fugitive" remote="tpope"
           path = ".vim/bundle/vim-fugitive" />
  <project name="vim-fetch" remote="kopischke"
           path = ".vim/bundle/vim-fetch" />

  <project name="gnus-init" remote="pi"
           path = ".emacs.d/lisp/gnus-init" />

  <project name="solarized-everything-css" remote="alphapapa"
           path = ".config/qutebrowser/solarized-everything-css" />

  <project name="bin" />
  <project name="z" remote="rupa"
           path="bin/z" />
</manifest>
